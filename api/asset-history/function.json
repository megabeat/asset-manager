{
  "scriptFile": "index.js",
  "entryPoint": "assetHistoryHandler",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get", "post", "delete"],
      "route": "assets/{assetId}/history/{historyId?}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
