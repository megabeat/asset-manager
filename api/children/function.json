{
  "scriptFile": "index.js",
  "entryPoint": "childrenHandler",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get", "post", "put", "delete"],
      "route": "children/{childId?}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
